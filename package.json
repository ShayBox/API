{
  "name": "api",
  "version": "0.0.0",
  "description": "My API written in Javascript running on NodeJS",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register .",
    "lint": "eslint src --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShayBox/API.git"
  },
  "author": {
    "email": "shayneehartford@gmail.com",
    "name": "Shayne Hartford (ShayBox)",
    "url": "https://shaybox.com/"
  },
  "dependencies": {
    "@types/apicache": "^1.2.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/sharp": "^0.25.0",
    "@types/ws": "^7.2.6",
    "apicache": "^1.5.3",
    "cors": "^2.8.5",
    "discord.js": "^12.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "sharp": "^0.25.4",
    "tplink-cloud-api": "^0.6.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-marine": "^7.1.2"
  },
  "eslintConfig": {
    "extends": "marine/node",
    "rules": {
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "variable",
          "format": [
            "camelCase",
            "UPPER_CASE"
          ]
        }
      ],
      "@typescript-eslint/no-misused-promises": "off",
      "@typescript-eslint/prefer-nullish-coalescing": "off",
      "@typescript-eslint/restrict-template-expressions": "off"
    }
  }
}
